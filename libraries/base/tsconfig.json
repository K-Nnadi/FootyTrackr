{
	"extends": "../../tsconfig.json",
	"main": "dist/index.js",
	"compilerOptions": {
		"outDir": "./dist",
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
	},
	"composite": true,
	"noEmit": false,
	"include": [
		"**/*"
	],
	"exclude": [
		"node_modules",
		"dist"
	]
}
